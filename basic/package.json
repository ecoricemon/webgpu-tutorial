{
  "author": "ecoricemon",
  "name": "basic",
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf dist pkg && webpack --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "build-release": "rimraf dist pkg && webpack --config webpack.prod.js",
    "test": "wasm-pack test --chrome --headless",
    "test-chrome": "wasm-pack test --chrome --headless",
    "test-firefox": "wasm-pack test --firefox --headless",
    "test-safari": "wasm-pack test --safari --headless",
    "clean": "rimraf dist node_modules pkg target"
  },
  "dependencies": {
    "lil-gui": "^0.18.2"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^5.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
